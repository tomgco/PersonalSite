!!! 5
html(lang="en")
	head
		title= title
		meta(name='author', content='Tom Gallacher - @tomgallacher89')
		meta(name='description', content='Tom Gallacher is a Software Engineer from Bournemouth, Uk. Proficient in PHP, Java, Javascript, SQL and Objective-C')
		meta(name='keywords', content='Tom Gallacher, tomgallacher89, nodejs, php, jobs, software engineer, php website developer, iOS developer')
		meta(charset='utf-8')
		meta(http-equiv='Content-Type', content='text/html; charset=utf-8')
		link(rel='stylesheet', href='/stylesheets/style.css')
	body
		input.command(type='text')
		#container
			#wrapper
				#info
					h1 Info
					ul
						li &gt;&nbsp;
							a(class=/^.\/*$/.test(currentUrl) ? 'active' : '', href='/') Terminal
						li &gt;&nbsp;
							a(class=/\/portfolio*/.test(currentUrl) ? 'active' : '', href='/portfolio') Portfolio
						li &gt;&nbsp;
							a(class=/\/projects*/.test(currentUrl) ? 'active' : '', href='/projects') Projects
						li &gt;&nbsp;
							a(class=/\/gzippo*/.test(currentUrl) ? 'active' : '', href='/gzippo') gzippo
						//li &gt;&nbsp;
						//	a(class=/\/blog*/.test(currentUrl) ? 'active' : '', href='/blog') Blog
					h1 Twitter
					#twitter
						!=partial('partials/tweets', globals)
				#main-content
					!= body
				#footer
					ul
						li.copyright &copy; Tom Gallacher
						li
							a#twitter(href='http://twitter.com/tomgallacher89', target='_blank', title='Tom Gallacher's Twitter Handle') @tomgallacher89
						li
							a#lastfm(href='http://www.last.fm/user/gxblast', target='_blank', title='Tom Gallacher's Last.fm') last.fm
						li
							a#github(href='http://github.com/tomgallacher', target='_blank', title='Tom Gallacher's Github') Github Profile
						li
							a#linkedin(href='http://uk.linkedin.com/pub/tom-gallacher/26/8a6/b94', target='_blank') Linkedin
		script(src='/js/jquery-1.5.2.min.js')
		script(src='/js/turtles.min.js')